FORM
# form_fill.py
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
import time
# Initialize Chrome driver
driver = webdriver.Chrome()
# Step 1: Open form page
driver.get("https://www.w3schools.com/html/html_forms.asp")
driver.maximize_window()
# Step 2: Find input fields by name
fname = driver.find_element(By.ID, "fname")
lname = driver.find_element(By.ID, "lname")
# Step 3: Clear and fill the form fields
fname.clear()
fname.send_keys("Sujata")
lname.clear()
lname.send_keys("Oak")
# Step 4: Scroll and click Submit button
driver.find_element(By.XPATH, "//input[@type='submit']").click()
# Step 5: Wait and print the new URL
time.sleep(5)
print("Form submitted successfully!")
print("Redirected URL:", driver.current_url)
# Step 6: Close browser
driver.quit()




YT SRCH
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
import time

# Set up Chrome WebDriver
driver = webdriver.Chrome()

# Open YouTube
driver.get("https://www.youtube.com")

# Wait for page to load
time.sleep(3)

# Find the search box
search_box = driver.find_element(By.NAME, "search_query")

# Enter search term
search_box.send_keys("Python Selenium tutorial")
search_box.send_keys(Keys.RETURN)

# Wait to load results
time.sleep(5)

# Print first 5 video titles
videos = driver.find_elements(By.ID, "video-title")
for i, video in enumerate(videos[:5]):
    print(f"{i+1}. {video.text}")

# Close the browser
driver.quit()




WIKI
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
import time

# Set up WebDriver (Chrome example)
driver = webdriver.Chrome()

# Open Wikipedia
driver.get("https://www.wikipedia.org/")

# Wait for page to load
time.sleep(2)

# Find the search input
search_box = driver.find_element(By.NAME, "search")

# Enter search term
search_box.send_keys("Python programming")
search_box.send_keys(Keys.RETURN)

# Wait for results page to load
time.sleep(3)

# Extract and print page title
page_title = driver.title
print("Page Title:", page_title)

# Close browser
driver.quit()



