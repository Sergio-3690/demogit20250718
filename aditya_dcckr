DOCKERFILE

ls Dockerfile Hello.java
nano Dockerfile

# Use official OpenJDK image
FROM openjdk:17

# Set working directory
WORKDIR /app

# Copy Java source code
COPY HelloWorld.java /app

# Compile Java program
RUN javac HelloWorld.java

# Run the program
CMD ["java", "HelloWorld"]


docker build -t sujatadocker2025/javaapp .
